{
    "name": "aelvan/imager",
    "description": "Image transforms gone wild",
    "type": "craft-plugin",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "image",
        "responsive",
        "transform",
        "srcset",
        "picture"
    ],
    "support": {
        "docs": "https://github.com/aelvan/Imager-Craft/blob/craft3/README.md",
        "issues": "https://github.com/aelvan/Imager-Craft/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "AndrÃ© Elvan",
            "homepage": "https://www.vaersaagod.no"
        }
    ],
    "require": {
        "php": "^8.0",
        "craftcms/cms": "^4.0",
        "mikehaertl/php-shellcommand": "^1.2",
        "tinify/tinify": ">=1.1.1",
        "ksubileau/color-thief-php": "^1.4.0",
        "ssnepenthe/color-utils": "~0.4",
        "imgix/imgix-php": "^2.1.0",
        "kraken-io/kraken-php": "^1.2",
        "aws/aws-sdk-php": "^3.0.0"
    },
    "autoload": {
        "psr-4": {
          "aelvan\\imager\\": "src/"
        }
    },
    "extra": {
        "name": "Imager",
        "handle": "imager",
        "schemaVersion": "2.0.0",
        "hasCpSettings": false,
        "hasCpSection": false,
        "changelogUrl": "https://raw.githubusercontent.com/aelvan/Imager-Craft/craft3/CHANGELOG.md",
        "components": {},
        "class": "aelvan\\imager\\Imager"
    },
    "require-dev": {
        "craftcms/rector": "dev-main"
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
